{
    "maxExprDepth": 40,
    "debug": {
        "showFPS": false,
        "virtualCursorShown": false,
        "printCursor": false,
        "printEvents": false,
        "jumpToSourceKeys": [ "Cmd+F12" ],
        "breakpoints": {
            "inference": false,
            "sugaring": false,
            "layout": false,
            "database": false,
            "naming": false
        }
    },
    "sugar": {
        "sugarsEnabled": {
            "assignmentParameters": true,
            "caseWithArgument": true,
            "composite": true,
            "fieldPuns": true,
            "fragment": true,
            "ifExpression": true,
            "labeledApply": true,
            "letExpression": true,
            "lightLambda": true,
            "literalText": true,
            "nullaryInject": true,
            "nullaryParameter": true,
            "parametersRecord": true
        }
    },
    "zoom": {
        "shrinkKeys"    : [ "Cmd+Minus" ],
        "enlargeKeys"   : [ "Cmd+Equal" ],
        "shrinkFactor"  : 1.1,
        "enlargeFactor" : 1.1
    },
    "export": {
        "exportPath"      : "export.json",
        "exportKeys"      : [ "Alt+E" ],
        "exportFancyKeys" : [ "Alt+Shift+J" ],
        "exportAllKeys"   : [ "Alt+Shift+E" ],
        "executeKeys"     : [ "Cmd+Alt+Shift+R" ],
        "importKeys"      : [ "Alt+I" ]
    },
    "pane": {
        "newDefinitionKeys" : [ "Alt+N", "Alt+L" ],
        "closeKeys"         : [ "Alt+W" ],
        "moveUpKeys"        : [ "Alt+Up" ],
        "moveDownKeys"      : [ "Alt+Down" ]
    },
    "versionControl": {
        "undoKeys"           : [ "Cmd+Z" ],
        "redoKeys"           : [ "Cmd+Y" ],
        "makeBranchKeys"     : [ "Cmd+S" ],
        "delBranchKeys"      : [ "Alt+O" ],
        "jumpToBranchesKeys" : [ "Cmd+F11" ]
    },
    "completion": {
        "jumpToNextKeys"  : [ "Tab" ],
        "resultCount"     : 5,
        "jumpToPrevKeys"  : [ "Shift+Tab" ]
    },
    "literal": {
        "startEditingKeys" : [ "Enter", "Shift+Right" ],
        "stopEditingKeys"  : [ "Escape", "Shift+Left" ]
    },
    "eval": {
        "prevScopeKeys": [ "Alt+Up" ],
        "nextScopeKeys": [ "Alt+Down" ]
    },
    "dirKeys": {
        "left": ["Left"],
        "right": ["Right"],
        "up": ["Up"],
        "down": ["Down"]
    },
    "orderDirKeys": {
        "left": ["Cmd+Left"],
        "right": ["Cmd+Right"],
        "up": ["Cmd+Up"],
        "down": ["Cmd+Down"]
    },
    "grid": {
        "moreLeft": [ "Home" ],
        "moreRight": [ "End" ],
        "leftMost": [ "Cmd+Home" ],
        "rightMost": [ "Cmd+End" ],
        "top": [ "PageUp" ],
        "bottom": [ "PageDown" ]
    },
    "textEdit": {
        "moveLeftWord": [ "Ctrl+Left" ],
        "moveRightWord": [ "Ctrl+Right" ],
        "home": [ "Home", "Ctrl+A" ],
        "end": [ "End", "Ctrl+E" ],
        "transposeLetters": [ "Ctrl+T" ],
        "deleteCharBackward": [ "Backspace" ],
        "deleteCharForward": [ "Delete" ],
        "deleteWordBackward": [ "Ctrl+W" ],
        "deleteWordForward": [ "Alt+D" ],
        "deleteToEndOfLine": [ "Ctrl+K" ],
        "deleteToBeginningOfLine": [ "Ctrl+U" ],
        "copy": [ "Cmd+C" ],
        "paste": [ "Cmd+V" ]
    },
    "searchMenu": {
        "openKeys": [ "Space", "Enter" ],
        "closeKeys": [ "Escape" ],
        "delSearchTermKeys": [ "Cmd+Delete" ],
        "menu": {
            "keysPickOption": [ "Enter" ],
            "keysPickOptionAndGotoNext": [ "Tab" ]
        }
    },
    "helpKeys": [ "F1", "Alt+H" ],
    "previousCursorKeys"       : [ "Alt+Left" ],
    "enterSubexpressionKeys"   : [ "Shift+Right" ],
    "actionKeys"               : [ "Space", "Enter" ],
    "delBackwardKeys"          : [ "Backspace" ],
    "quitKeys"                 : [ "Cmd+Q" ],
    "changeThemeKeys"          : [ "Alt+T" ],
    "changeLanguageKeys"       : [],
    "addNextParamKeys"         : [ "Space" ],
    "healKeys"                 : [ "Space", "Enter" ],
    "recordOpenKeys"           : [ "Alt+O" ],
    "recordCloseKeys"          : [ "Alt+O" ],
    "caseOpenKeys"             : [ "Alt+O" ],
    "extractKeys"              : [ "Cmd+X", "Cmd+Up" ],
    "moveLetInwardKeys"        : [ "Cmd+Down" ],
    "inlineKeys"               : [ "Cmd+V" ],
    "nextAnnotationModeKeys"   : [ "F7" ],
    "leaveSubexpressionKeys"   : [ "Shift+Left" ],
    "recordAddFieldKeys"       : [ "Cmd+A", "Comma" ],
    "caseAddAltKeys"           : [ "Cmd+A", "Comma" ],
    "delForwardKeys"           : [ "Delete", "Alt+Delete" ],
    "replaceParentKeys"        : [ "Cmd+Delete", "Cmd+Backspace" ],
    "letAddItemKeys"           : [ "Alt+L" ],
    "injectValueKeys"          : [ "Space" ],
    "jumpToDefinitionKeys"     : [ "Enter" ]
}
